{
  "name": "behappy/invoice-plugin",
  "type": "sylius-plugin",
  "description": "A plugin to generate invoices in Sylius",
  "license": "MIT",
  "authors": [
    {
      "name": "BeHappy Communication",
      "role": "Organization",
      "homepage": "http://www.behappycom.com",
      "email": "projets@behappycom.com"
    },
    {
      "name": "St√©phane DECOCK",
      "role": "Developer",
      "homepage": "http://www.behappycom.com",
      "email": "s.decock@behappycom.com"
    }
  ],
  "require": {
    "php": "^7.1",

    "sylius/sylius": "^1.1",
    "knplabs/knp-snappy-bundle": "^1.5",
    "behappy/company-data-plugin": "^1.0"
  },
  "require-dev": {
    "behat/behat": "^3.3",
    "behat/mink": "^1.7",
    "behat/mink-browserkit-driver": "^1.3",
    "behat/mink-extension": "^2.2",
    "behat/mink-selenium2-driver": "^1.3",
    "friends-of-behat/context-service-extension": "^1.0",
    "friends-of-behat/cross-container-extension": "^1.0",
    "friends-of-behat/service-container-extension": "^1.0",
    "friends-of-behat/symfony-extension": "^1.0",
    "friends-of-behat/variadic-extension": "^1.0",
    "lakion/mink-debug-extension": "^1.2.3",
    "phpspec/phpspec": "^3.2",
    "phpstan/phpstan-shim": "^0.9.2",
    "phpunit/phpunit": "^6.5",
    "se/selenium-server-standalone": "^2.52",
    "sylius-labs/coding-standard": "^1.0",
    "symplify/easy-coding-standard": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "BeHappy\\SyliusInvoicePlugin\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/Application/app/AppKernel.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
